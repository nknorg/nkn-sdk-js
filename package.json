{
  "name": "nkn-sdk",
  "version": "1.3.3",
  "description": "NKN client and wallet SDK",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./index.d.ts"
    },
    "./client/*": {
      "import": "./lib/client/*.js",
      "require": "./lib/client/*.js"
    },
    "./common/*": {
      "import": "./lib/common/*.js",
      "require": "./lib/common/*.js"
    },
    "./multiclient/*": {
      "import": "./lib/multiclient/*.js",
      "require": "./lib/multiclient/*.js"
    },
    "./wallet/*": {
      "import": "./lib/wallet/*.js",
      "require": "./lib/wallet/*.js"
    },
    "./worker/*": {
      "import": "./lib/worker/*.js",
      "require": "./lib/worker/*.js"
    }
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "babel src --out-dir lib && browserify lib/index.js -s nkn --outfile dist/nkn.js && uglifyjs dist/nkn.js -c -m -o dist/nkn.min.js",
    "docs": "documentation build src/*/*.js -f html -o docs --sort-order alpha --shallow",
    "prepare": "yarn flow && yarn build && yarn docs",
    "test": "jest",
    "pb": "protoc --js_out=import_style=commonjs,binary:. src/common/pb/*.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nknorg/nkn-sdk-js.git"
  },
  "author": "NKN",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nknorg/nkn-sdk-js/issues"
  },
  "homepage": "https://github.com/nknorg/nkn-sdk-js#readme",
  "keywords": [
    "nkn",
    "p2p",
    "peer-to-peer",
    "distributed",
    "decentralized",
    "blockchain",
    "realtime",
    "network",
    "networking",
    "streaming"
  ],
  "dependencies": {
    "@nkn/ncp": "^1.1.1",
    "axios": "^0.28.0",
    "base-x": "^3.0.7",
    "buffer": "^5.4.3",
    "core-js-pure": "^3.6.4",
    "crypto-js": "^4.2.0",
    "decimal.js": "^10.2.0",
    "ed2curve": "^0.3.0",
    "google-protobuf": "^3.11.2",
    "heap": "^0.2.6",
    "isomorphic-ws": "^4.0.1",
    "libsodium-wrappers": "^0.7.6",
    "memory-cache": "^0.2.0",
    "pako": "^1.0.11",
    "scrypt-js": "^3.0.0",
    "tweetnacl": "^1.0.3",
    "webworkify": "^1.5.0",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-export-default-from": "^7.16.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-flow": "^7.16.5",
    "browserify": "^16.5.0",
    "documentation": "^12.1.4",
    "flow-bin": "^0.118.0",
    "jest": "^25.1.0",
    "uglify-es": "^3.3.9"
  }
}
